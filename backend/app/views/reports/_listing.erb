<table class="report-listing">
  <thead>
    <tr>
      <% report.headers.each do |heading| %>
        <th><%= heading %></th>
      <% end %>
    </tr>
  </thead>
  <tbody>
    <% report.each do |record| %>
      <tr>
        <% report.headers.each do |heading| %>
          <td>
            <%= CGI.escapeHTML(record[heading].to_s) %>
          </td>
        <% end %>
      </tr>
    <% end %>
  </tbody>
</table>